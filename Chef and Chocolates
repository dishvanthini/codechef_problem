#include <stdio.h>
int main() {
    int t,five ,ten,tot ;
    scanf("%d", &t);

    while (t--) {
       int x, y, z;
       scanf("%d %d %d", &x, &y, &z);
       five=5*x;//
       ten=y*10;
       tot=five+ten;
       printf("%d\n",tot/z);
    }

}
//Chef has X 5 rupee coins and Y 10 rupee coins. Chef goes to a shop to buy chocolates for Chefina where each chocolate costs Z rupees. Find the maximum number of chocolates that Chef can buy for Chefina.
