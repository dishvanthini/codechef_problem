#include <stdio.h>

int main() {
    int x,y,z,res;
        int t;
        scanf("%d",&t);
while(t--){

scanf("%d %d",&x,&y) ;  
z=x*y;
res=(z>=100)?(z/100):0;
printf("%d\n",res);
}

return 0;
}





