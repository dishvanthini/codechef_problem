#include <stdio.h>

int main() {
    int t;
    char s1[10],s2[10];
    scanf("%d",&t);
    
    while(t--){
    scanf("%s", s1);     
    scanf("%s", s2);    
    
    for (int i=0;i<5;i++)
    {
    if (s1[i]==s2[i])
        printf("G");
    else
        printf("B");
    }
    printf("\n");
}        
return 0 ;
        
}

