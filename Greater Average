#include <stdio.h>

int main() 
{
	int t ;
	float a,b,c;
	float avg;
	scanf("%d",&t);
	while(t--)
	{
	    scanf("%f %f %f ",&a,&b,&c);
	    avg=(a+b)/2.0;
	    if(avg>c){ //6.5>6  yes 
	        printf("YES\n");
	    }else if(avg=c){ //6=6 --no 
	        printf("NO\n");
	    }else{
	       printf("NO\n");
	    }
	}

return 0;

}

